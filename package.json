{
  "name": "document-management",
  "version": "1.0.0",
  "description": "Document Management System is a react redux application based on RESTful API for users to create and manage documents giving different privileges based on user roles and managing authentication using JWT.",
  "main": "index.js",
  "scripts": {
    "start": "babel-watch ./server.js",
    "lint": "esw webpack.config.* client server",
    "build": "babel client -d lib",
    "test": "NODE_ENV=test npm run db:test:seed && npm run cover",
    "test:backend": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --opts mocha.opts server/test/**/*.spec.js --compilers js:babel-register",
    "mocha:test": "NODE_ENV=test npm run db:test:seed && mocha --reporter spec ./tools/testSetup.js \"./**/test/**/*.spec.js\" --timeout 60000",
    "cover": "NODE_ENV=test nyc --reporter=html --reporter=text _mocha --reporter spec ./server/test/testSetup.js \"./**/test/**/*.spec.js\" --timeout=60000",
    "test:watch": "npm run mocha:test -- --watch",
    "test:all": "npm run db:test:seed && npm run thetest:watch",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test babel-watch ./server/test/helper/seed-helper",
    "db:seed": "babel-watch ./server/test/helper/seed-helper",
    "coveralls": "nyc report --reporter=text-lcov |  coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-fmustapha/document-management.git"
  },
  "keywords": [
    "'document",
    "management'",
    "'REACT'",
    "'REDUX'",
    "'postgresql'"
  ],
  "author": "Faith Mustapha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-fmustapha/document-management/issues"
  },
  "homepage": "https://github.com/andela-fmustapha/document-management#readme",
